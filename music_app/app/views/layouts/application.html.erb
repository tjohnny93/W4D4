<!DOCTYPE html>
<html>
  <head>
    <title>MusicApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <h1> Music App </h1>
      <% if logged_in? %>
        
        <p>Logged in as: <%= current_user.email %> </p>

      
      <% end %>
    <%= yield %>
  </body>
</html>
